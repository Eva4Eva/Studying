def resp = new XmlHolder (messageExchange.responseContentAsXml)
def reg = new XmlHolder (messageExchange.requestContentAsXml)
resp.namespaces ["s"]="http://www.w3.org/2003/05/soap-envelope" 
resp.namespaces ["a"]="http://www.w3.org/2005/08/addressing" 
resp.namespaces ["u"]="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
resp.namespaces ["b"]="RGS.UFO"

//берем ответ в виде xml
def XmlResponse = messageExchange.getResponseContentAsXml()
def records = new XmlSlurper().parseText(XmlResponse)

//выбираем ContractId
String calculationId = records."Body"."CalculateResponse"."CalculateResult"."Data"."CalculationId".toString()

//выбираем IsSuccess
String isSuccess = new String (resp.getNodeValue('//b:IsSuccess'))

